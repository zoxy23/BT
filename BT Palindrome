//B11:In ra các số Palindrome (có 3 hoặc có 4 chữ số) mà có 
//tổng các chữ số chia hết cho 5.
int pa(int n){
	int a=0,b=n;
	while(n != 0){
		a=a*10+n%10;n/=10;
	}return(a==b);
}
int sum(int n){
	int sum = 0;
	while(n > 0){
		sum += n % 10;
		n /= 10;
	}
}
main(){
	int i,n;printf("Nhap N:");scanf("%d",&n);
	int tong=0;
	printf("Cac so Palindrome co tong cac chu so chia het cho 5:\n");
	for(i=100;i<=n;i++){
		if(pa(i) && sum(i) % 5 == 0){
			printf("%d ",i);
		}
	}
}
//*********************************************************************************************************************************
