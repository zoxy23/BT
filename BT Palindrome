//B2:Nếu không phải Palindrome,in ra các ước của nó
int pa(int n){
	int a=0,b=n;
	while(n>0){
		a=a*10 + (n%10);
		n /= 10;
	}return(a==b);
}

main(){
	int n,i;printf("Nhap n:");scanf("%d",&n);
	if(pa(n)){
		printf("N la Palindrome");
	}else{
		printf("N khong la Palindrome\n");
		printf("Uoc cua N la:");
		for(i=1;i<=n;i++){
			if(n % i == 0){
				printf("%d ",i);
			}
		}printf("\n");
	}
}


